{
  "name": "@elmijo/vue-timepicker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name VueTimePicker .src/components/TimePicker.js",
    "test": "vue-cli-service tests",
    "e2e": "vue-cli-service e2e",
    "e2e:open": "vue-cli-service e2e:open",
    "lint": "vue-cli-service lint",
    "test:unit": "TEST_ENV=node mocha-webpack --webpack-config test/webpack.test.config.js test/unit/**/*.spec.js --recursive --require test/mocha.setup.js"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "webpack": "^3.6.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
